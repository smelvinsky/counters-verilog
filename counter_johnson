#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x56363c0fcbf0 .scope module, "counter_johnson_tb" "counter_johnson_tb" 2 11;
 .timescale -9 -12;
v0x56363c11d340_0 .var "Clock", 0 0;
v0x56363c11d400_0 .net "Out", 3 0, L_0x56363c11d790;  1 drivers
v0x56363c11d4d0_0 .var "Reset", 0 0;
v0x56363c11d5d0_0 .var "Up_down", 0 0;
v0x56363c11d6a0_0 .var/i "i", 31 0;
S_0x56363c0fcd70 .scope module, "uut" "counter" 2 25, 3 7 0, S_0x56363c0fcbf0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clock"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "up_down"
    .port_info 3 /OUTPUT 4 "out"
L_0x56363c11d790 .functor BUFZ 4, v0x56363c11d030_0, C4<0000>, C4<0000>, C4<0000>;
v0x56363c0fcfb0_0 .net "clock", 0 0, v0x56363c11d340_0;  1 drivers
v0x56363c11cf50_0 .net "out", 3 0, L_0x56363c11d790;  alias, 1 drivers
v0x56363c11d030_0 .var "out_tmp", 3 0;
v0x56363c11d0f0_0 .net "reset", 0 0, v0x56363c11d4d0_0;  1 drivers
v0x56363c11d1b0_0 .net "up_down", 0 0, v0x56363c11d5d0_0;  1 drivers
E_0x56363c0f4210/0 .event edge, v0x56363c11d0f0_0;
E_0x56363c0f4210/1 .event posedge, v0x56363c0fcfb0_0;
E_0x56363c0f4210 .event/or E_0x56363c0f4210/0, E_0x56363c0f4210/1;
    .scope S_0x56363c0fcd70;
T_0 ;
    %wait E_0x56363c0f4210;
    %load/vec4 v0x56363c11d0f0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x56363c11d030_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x56363c11d1b0_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x56363c11d030_0;
    %parti/s 3, 0, 2;
    %load/vec4 v0x56363c11d030_0;
    %parti/s 1, 3, 3;
    %inv;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x56363c11d030_0, 0, 4;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x56363c11d030_0;
    %parti/s 1, 0, 2;
    %inv;
    %load/vec4 v0x56363c11d030_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x56363c11d030_0, 0, 4;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56363c0fcbf0;
T_1 ;
    %vpi_call 2 29 "$display", "Up/down Johnson's counter testbench...\012" {0 0 0};
    %vpi_call 2 30 "$dumpfile", "counter_1_test.vcd" {0 0 0};
    %vpi_call 2 31 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56363c11d340_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x56363c11d6a0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56363c11d5d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56363c11d4d0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56363c11d4d0_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56363c11d4d0_0, 0, 1;
    %delay 20000, 0;
T_1.0 ;
    %load/vec4 v0x56363c11d6a0_0;
    %cmpi/s 64, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x56363c11d6a0_0;
    %cmpi/e 32, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56363c11d5d0_0, 0, 1;
    %vpi_call 2 46 "$display", "Up/Down change" {0 0 0};
T_1.2 ;
    %delay 10000, 0;
    %load/vec4 v0x56363c11d340_0;
    %inv;
    %store/vec4 v0x56363c11d340_0, 0, 1;
    %load/vec4 v0x56363c11d6a0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x56363c11d6a0_0, 0, 32;
    %vpi_call 2 51 "$display", "Counter out: %b\011time = %d", v0x56363c11d400_0, $time {0 0 0};
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 54 "$display", "Done..." {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_johnson_tb.v";
    "./counter_johnson.v";
